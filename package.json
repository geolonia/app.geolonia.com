{
  "name": "app.geolonia.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "@wordpress/i18n": "^3.6.0",
    "amazon-cognito-identity-js": "^3.0.15",
    "chart.js": "^2.8.0",
    "interweave": "^11.1.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "prettier-cli": "^0.1.0",
    "react": "^16.9.0",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": ". ./.env.development && react-scripts start",
    "build": ". ./.env.development && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "xgettext": "find . -name '*.tsx' | xargs xgettext --keyword=__ -k_x:1,2c -k_n:1,2 -k_nx:1,2 -L JavaScript -o src/lang/lang.pot --from-code=utf-8",
    "msgmerge": "msgmerge -U src/lang/ja.po src/lang/lang.pot --lang=ja",
    "po2json": "po2json src/lang/ja.po src/lang/ja.json -f jed",
    "i18n": "npm run xgettext && npm run msgmerge",
    "format": "prettier './src/**/*.ts' './src/**/*.tsx' --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/mapbox-gl": "^0.54.3",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/wordpress__i18n": "^3.4.0",
    "@types/react-redux": "^7.1.2",
    "po2json": "^1.0.0-alpha"
  }
}
