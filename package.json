{
  "name": "app.geolonia.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "@material-ui/core": "^4.4.3",
    "@material-ui/icons": "^4.4.3",
    "@wordpress/i18n": "^3.6.0",
    "amazon-cognito-identity-js": "^3.0.15",
    "chart.js": "^2.8.0",
    "interweave": "^11.2.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "react": "^16.9.0",
    "react-chartjs-2": "^2.8.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.2.0",
    "redux": "^4.0.4",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": ". ./.env.development && react-scripts start",
    "build": ". ./.env.development && react-scripts build",
    "build:ci": "react-scripts build && npm run netlify:header",
    "netlify:header": "echo \"/*\n  Basic-Auth: $BASIC_AUTH_ID:$BASIC_AUTH_PASS\" > build/_headers",
    "test": "react-scripts test",
    "lint": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js .",
    "eject": "react-scripts eject",
    "makepot": "node_modules/@babel/cli/bin/babel.js ./src -x '.tsx' --config-file ./.babelrc.i18n",
    "msgmerge": "msgmerge -U src/lang/ja.po src/lang/lang.pot --lang=ja",
    "po2json": "po2json src/lang/ja.po src/lang/ja.json -f jed",
    "i18n": "npm run makepot && npm run msgmerge",
    "format": "prettier './src/**/*.ts' './src/**/*.tsx' --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@testing-library/react": "^9.2.0",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "24.0.18",
    "@types/mapbox-gl": "^0.54.3",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-tabs": "^2.3.1",
    "@types/styled-components": "^4.4.0",
    "@types/uuid": "^3.4.6",
    "@types/wordpress__i18n": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "@wordpress/babel-plugin-makepot": "^3.2.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "node-sass": "^4.12.0",
    "po2json": "^1.0.0-alpha",
    "prettier": "^1.18.2",
    "prettier-cli": "^0.1.0",
    "react-test-renderer": "^16.11.0",
    "typescript": "3.5.3",
    "uuid": "^3.3.3"
  }
}
