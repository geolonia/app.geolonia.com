{
  "name": "app.geolonia.com",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@mapbox/geojson-extent": "^0.3.2",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.5.0",
    "amazon-cognito-identity-js": "^3.2.0",
    "chart.js": "^2.9.3",
    "clipboard-polyfill": "^2.8.6",
    "interweave": "^11.2.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "query-string": "^6.11.1",
    "react": "^17.0.0",
    "react-chartjs-2": "^2.8.0",
    "react-color": "^2.17.3",
    "react-dom": "^17.0.0",
    "react-dropzone": "^11.3.4",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "redux": "^4.0.4",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": ". ./.env.production && react-scripts start",
    "build": "react-scripts build",
    "build:ci": "react-scripts build && bin/postbuild.sh",
    "cname": "echo $CNAME > ./build/CNAME",
    "test": "react-scripts test",
    "lint": "eslint --ext .tsx --ext .ts --ext .jsx --ext .js .",
    "eject": "react-scripts eject",
    "makepot": "node_modules/@babel/cli/bin/babel.js ./src -x '.tsx' -x '.ts' --config-file ./.babelrc.i18n",
    "msgmerge": "msgmerge -U src/lang/ja.po src/lang/lang.pot --lang=ja",
    "po2json": "po2json src/lang/ja.po src/lang/ja.json -f jed && prettier ./src/lang/ja.json --write",
    "validate:jed": "node ./bin/validate_jed.js",
    "i18n": "npm run makepot && npm run msgmerge",
    "format": "prettier './src/**/*.ts' './src/**/*.tsx' --write"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.7.2",
    "@testing-library/react": "^9.3.2",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.7",
    "@types/mapbox-gl": "^1.11.1",
    "@types/node": "^14.0.26",
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-tabs": "^2.3.2",
    "@types/styled-components": "^5.1.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@wordpress/babel-plugin-makepot": "^3.7.0",
    "@wordpress/i18n": "^3.14.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "node-sass": "^4.14.1",
    "po2json": "^1.0.0-alpha",
    "prettier": "^1.19.1",
    "prettier-cli": "^0.1.0",
    "react-test-renderer": "^16.12.0",
    "typescript": "^4.0.0",
    "uuid": "^3.3.3"
  }
}
